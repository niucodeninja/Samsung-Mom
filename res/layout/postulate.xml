<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/AbsoluteLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/postulate" >

    <!-- Registration Form -->

    <EditText
        android:id="@+id/pos_why_be_mother_of_year"
        android:layout_width="336dp"
        android:layout_height="100dp"
        android:layout_marginBottom="20dip"
        android:layout_marginTop="5dip"
        android:layout_x="90dp"
        android:layout_y="460dp"
        android:inputType="textAutoComplete"
        android:singleLine="false" />

    <EditText
        android:id="@+id/pos_name"
        android:layout_width="258dp"
        android:layout_height="40dp"
        android:layout_x="157dp"
        android:layout_y="272dp"
        android:inputType="textPersonName"
        android:singleLine="true" />

    <Button
        android:id="@+id/btnPostulate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="94dp"
        android:layout_y="569dp"
        android:background="@drawable/up_photo_btn" />

    <EditText
        android:id="@+id/pos_smart_mother_sentence"
        android:layout_width="307dp"
        android:layout_height="40dp"
        android:layout_x="94dp"
        android:layout_y="384dp"
        android:inputType="textAutoComplete"
        android:singleLine="false" />

    <EditText
        android:id="@+id/pos_tipical_sentence"
        android:layout_width="315dp"
        android:layout_height="40dp"
        android:layout_x="94dp"
        android:layout_y="332dp"
        android:inputType="textAutoComplete"
        android:singleLine="false" >

        <requestFocus />
    </EditText>

</AbsoluteLayout>